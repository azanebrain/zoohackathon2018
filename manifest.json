{
  "name": "Social Impact",
  "version": "1.0",
  "description": "Gently notify the user that these items are harmful to XYZ.",
  "permissions": ["<all_urls>", "activeTab", "declarativeContent", "storage"],
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
  "options_page": "options.html",
  "page_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/palm.crop.16.png",
      "32": "images/palm.crop.32.png",
      "48": "images/palm.crop.48.png",
      "128": "images/palm.crop.128.png"
    }
  },
  "icons": {
    "16": "images/palm.crop.16.png",
    "32": "images/palm.crop.32.png",
    "48": "images/palm.crop.48.png",
    "128": "images/palm.crop.128.png"
  },
  "manifest_version": 2
}
